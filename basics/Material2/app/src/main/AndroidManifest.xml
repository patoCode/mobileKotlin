<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApp"
        tools:targetApi="31">
        <activity
            android:name=".mui3.widgets.MenuWidgetActivity"
            android:exported="false" />
        <activity
            android:name=".mui3.widgets.ProgressbarActivity"
            android:exported="false" />
        <activity
            android:name=".mui3.navigators.DrawerActivity"
            android:exported="false" />
        <activity
            android:name=".mui3.forms.DropdownActivity"
            android:exported="false" />
        <activity
            android:name=".mui3.containers.MenuContainerActivity"
            android:exported="false" />
        <activity
            android:name=".mui3.forms.DatepickerActivity"
            android:exported="false" />
        <activity
            android:name=".mui3.forms.CheckboxActivity"
            android:exported="false" />
        <activity
            android:name=".mui3.forms.MenuFormActivity"
            android:exported="false" />
        <activity
            android:name=".mui3.forms.ButtonActivity"
            android:exported="false" />
        <activity
            android:name=".mui3.navigators.MenuNavigatorActivity"
            android:exported="false" />
        <activity
            android:name=".mui3.navigators.BottomNavActivity"
            android:exported="false" />
        <activity
            android:name=".mui3.navigators.BottomAppbarActivity"
            android:exported="false" />
        <activity
            android:name=".mui3.navigators.AppbarActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>